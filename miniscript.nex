
/\n+/ {
  util.LogToken("NEWLINE\n")
  util.LogTrace("Lexed newline", 1)
  return NEWLINE
}

/<script type=\"text\/JavaScript\">/ {
  util.LogToken("SCRIPT_TAG_START ")
  util.LogTrace("Lexed start script tag", 1)
  return SCRIPT_TAG_START
}

/<\/script>/ {
  util.LogToken("SCRIPT_TAG_END ")
  util.LogTrace("Lexed end script tag", 1)
  return SCRIPT_TAG_END
}

/document\.write/ {
  util.LogToken("DOCUMENT_WRITE ")
  util.LogTrace("Lexed document write function call", 1)
  return DOCUMENT_WRITE
}

/var[ \t]/ {
  util.LogToken("VARDEF ")
  util.LogTrace("Lexed var keyword", 1)
  return VARDEF
}

/[ \t]+/ {
  util.LogToken("")
}

/[0-9]+/ {
  util.LogToken("INTEGER ")
  util.LogTokenData("(" + yylex.Text() + ") ")
  util.LogTrace("Lexed integer " + yylex.Text(), 1)
  return INTEGER
}

/[A-Za-z][A-Za-z0-9\.]*/ {
  util.LogToken("IDEN ")
  util.LogTokenData("(" + yylex.Text() + ") ")
  util.LogTrace("Lexed identifier " + yylex.Text(), 1)
  return IDENTIFIER
}

/\"(\\.|[^\\"\n])*\"/ {
  util.LogToken("STRING ")
  util.LogTokenData("(" + yylex.Text() + ") ")
  util.LogTrace("Lexed string " + yylex.Text(), 1)
  return STRING
}

/=/ {
  util.LogToken("EQUAL ")
  util.LogTrace("Lexed equal sign", 1)
  return EQUAL
}

/{/ {
  util.LogToken("LBRACE ")
  util.LogTrace("Lexed left brace", 1)
  return LBRACE
}

/}/ {
  util.LogToken("RBRACE ")
  util.LogTrace("Lexed right brace", 1)
  return RBRACE
}

/\(/ {
  util.LogToken("LPAREN ")
  util.LogTrace("Lexed left paren", 1)
  return LPAREN
}

/\)/ {
  util.LogToken("RPAREN ")
  util.LogTrace("Lexed right paren", 1)
  return RPAREN
}

/,/ {
  util.LogToken("COMMA ")
  util.LogTrace("Lexed comma", 1)
  return COMMA
}

/\+/ {
  util.LogToken("PLUS ")
  util.LogTrace("Lexed plus", 1)
  return PLUS
}

/-/ {
  util.LogToken("MINUS ")
  util.LogTrace("Lexed minus", 1)
  return MINUS
}

/\*/ {
  util.LogToken("MULT ")
  util.LogTrace("Lexed mult", 1)
  return MULT
}

/\// {
  util.LogToken("DIVIDE ")
  util.LogTrace("Lexed divide", 1)
  return DIVIDE
}

/:/ {
  util.LogToken("COLON ")
  util.LogTrace("Lexed colon", 1)
  return COLON
}

/;/ {
  util.LogToken("SEMICOLON ")
  util.LogTrace("Lexed semicolon", 1)
  return SEMICOLON
}

/./ {
  return int(yylex.Text()[0])
}

